{
  "name": "dragnsurvey/openai",
  "description": "Laravel Package to interract with OpenAI API",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "romanstec",
      "email": "roman.stec@dragnsurvey.com"
    }
  ],
  "require": {
    "php": "^8.1.0",
    "guzzlehttp/guzzle": "^7.7.0",
    "laravel/framework": "^9.46.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dragnsurvey\\OpenAi\\OpenAiServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Dragnsurvey\\OpenAi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dragnsurvey\\OpenAi\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "*"
  }
}
